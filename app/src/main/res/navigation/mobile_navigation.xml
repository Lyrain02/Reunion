<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mytest.ui.square.SquareFragment"
        android:label="广场"
        tools:layout="@layout/fragment_square" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.mytest.ui.send.SendFragment"
        android:label="发布公告"
        tools:layout="@layout/fragment_send" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.mytest.ui.myinfo.MyinfoFragment"
        android:label="我的信息"
        tools:layout="@layout/fragment_myinfo" />

</navigation>